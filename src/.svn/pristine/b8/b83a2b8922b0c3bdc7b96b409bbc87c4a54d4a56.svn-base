
public class Fabrique {
	public Character creerUnite(TypeUnite type)
    {
        Character unite = null;
        switch(type)
        {
            case DROID:
            	unite = new Droid();
            	break;
            	
            case CLONE:
            	unite = new Clone();
            	break;
            	
            case SOLDAT:
            	unite = new Soldat();
            	break;
        }
        return unite;
    }
}